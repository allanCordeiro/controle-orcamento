							"    pm.response.to.have.status(201);",
							"});",
							"",
							"pm.test(\"Validating body response\", function() {",
							"    var result = pm.response.json();",
							"    pm.expect(result.descricao).to.equals('cartão de crédito');        ",
							"});"
